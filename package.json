{
  "name": "actor-boilerplate",
  "version": "0.0.1",
  "description": "A starting point for web apps based on the actor model",
  "scripts": {
    "copy-static": "cpx \"static/**\" dist",
    "gzip": "gzip -k9nr dist",
    "build": "rollup -c && npm run copy-static && npm run gzip",
    "serve": "http-server dist -p 8989 -c0 -g"
  },
  "author": "Surma <surma@google.com>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "actor-helpers": "git+https://github.com/PolymerLabs/actor-helpers.git#master",
    "cpx": "^1.5.0",
    "preact": "^10.3.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.31.1",
    "rollup-plugin-loadz0r": "^0.7.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "rollup-plugin-typescript2": "^0.26.0",
    "typescript": "^3.8.2"
  },
  "devDependencies": {
    "@types/jest": "^25.1.3",
    "@types/ramda": "^0.26.42",
    "eslint": "^6.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslintrc": "^1.0.6",
    "firebase-cli": "^1.2.0",
    "http-server": "^0.12.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "ramda": "^0.27.0",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-index-html": "^1.9.2",
    "rollup-plugin-live-server": "^1.0.3",
    "ts-jest": "^25.2.1",
    "xstate": "^4.7.8"
  }
}
